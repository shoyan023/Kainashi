  <div class="sec6">
  <div class="register_bg">
  <div class="register_wrapper">
    <p class="page_title">REGISTER</p>
    <p class="register_navi">登録はメールアドレスのみ。今すぐ思考の旅に出かけよう。</p>

   <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
   <%= devise_error_messages! %>

  <ul>
    <li>
       <span><%= f.label :EMAIL %></span>
       <%= f.email_field :email, :placeholder => 'メールアドレスを入力', :class => 'form' %>
   </li>
   <li>
       <span><%= f.label :NAME %></span>
       <%= f.text_field :username, :placeholder => '文字の半角英数字', autofocus: true, :class => 'form' %>
       
   </li>
   <li>
       <span><%= f.label :PASSWORD %></span>
       <%= f.password_field :password, :placeholder => '8 〜 16 文字の半角英数字', autocomplete: "off", :class => 'form' %>
   </li>
   <li>
       <span><%= f.label :CONFIRM %></span>
       <%= f.password_field :password_confirmation, :placeholder => 'パスワードを再入力してください', autocomplete: "off", :class => 'form' %>
   </li>

    <div class="other_features">
    <p class="agreement"><input type="checkbox"><a href="" class="agreement_link">利用規約</a>に同意する</p>
      <div class="button"><%= f.submit "はじめる（無料）", :class => 'button button_register' %></div>
    </div>
   <% end %>

</div>